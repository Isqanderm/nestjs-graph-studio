{
  "name": "nestjs-graph-studio-example",
  "version": "0.1.0",
  "private": true,
  "description": "Example NestJS application demonstrating nestjs-graph-studio features including DI graph visualization, request tracing, and circular dependency detection",
  "scripts": {
    "postinstall": "bash setup.sh",
    "build": "tsc",
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register main.ts",
    "start:dev": "ts-node -r tsconfig-paths/register main.ts",
    "start:debug": "ts-node --inspect -r tsconfig-paths/register main.ts",
    "start:prod": "node dist/main.js",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "echo \"No tests configured for example app\"",
    "test:watch": "echo \"No tests configured for example app\"",
    "test:cov": "echo \"No tests configured for example app\"",
    "test:debug": "echo \"No tests configured for example app\"",
    "test:e2e": "echo \"No tests configured for example app\"",
    "test:orders": "bash test-orders.sh",
    "test:pipes": "bash test-pipes.sh",
    "create:orders": "bash create-test-orders.sh"
  },
  "dependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-express": "^10.3.0",
    "@nestjs/platform-fastify": "^10.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "express": "^4.18.2",
    "fastify": "^4.25.2",
    "nestjs-graph-studio": "file:..",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

