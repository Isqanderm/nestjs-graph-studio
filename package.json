{
  "name": "nestjs-graph-studio",
  "version": "0.1.2",
  "description": "Local DevTools for NestJS applications with DI graph visualization",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**",
    "README.md",
    "LICENSE",
    "CHANGELOG.md"
  ],
  "scripts": {
    "build:ui": "vite build --config ui/vite.config.ts && node scripts/copy-ui.js",
    "build:lib": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build": "npm run build:lib && npm run build:ui",
    "dev:ui": "vite --config ui/vite.config.ts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "example": "cd example && npm start",
    "example:install": "cd example && npm install",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest run --config ui/vitest.config.ts",
    "test:ui:watch": "vitest --config ui/vitest.config.ts",
    "test:ui:coverage": "vitest run --coverage --config ui/vitest.config.ts",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:chromium": "playwright test --project=chromium",
    "test:e2e:debug": "playwright test --debug",
    "test:all": "npm run test && npm run test:ui && npm run test:e2e",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.ts\" \"ui/src/**/*.{ts,tsx}\"",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "nestjs",
    "devtools",
    "dependency-injection",
    "graph",
    "visualization",
    "tracing",
    "debugging",
    "monitoring"
  ],
  "author": "Aleksandr Melnik <aleksandr.melnik.personal@gmail.com> (https://github.com/Isqanderm)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Isqanderm/nestjs-graph-studio.git"
  },
  "bugs": {
    "url": "https://github.com/Isqanderm/nestjs-graph-studio/issues"
  },
  "homepage": "https://github.com/Isqanderm/nestjs-graph-studio#readme",
  "peerDependencies": {
    "@nestjs/common": "^9.0.0 || ^10.0.0",
    "@nestjs/core": "^9.0.0 || ^10.0.0",
    "reflect-metadata": "^0.1.13 || ^0.2.0"
  },
  "dependencies": {
    "@types/dagre": "^0.7.53",
    "dagre": "^0.8.5",
    "html-to-image": "^1.11.13",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@nestjs/common": "^10.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/testing": "^10.4.20",
    "@playwright/test": "^1.56.1",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@tailwindcss/postcss": "^4.1.16",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.6.1",
    "autoprefixer": "^10.4.21",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.38.0",
    "happy-dom": "^20.0.10",
    "jsdom": "^27.0.1",
    "postcss": "^8.5.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.0",
    "reflect-metadata": "^0.2.1",
    "supertest": "^7.1.4",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.16",
    "tailwindcss-animate": "^1.0.7",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.10",
    "vitest": "^1.1.0",
    "zustand": "^4.4.7"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
